<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mall.naiqiao.mylibrary">
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 读取联系人权限 -->
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.canyinghao.canphotos.CanPhotoActivity" />
        <activity android:name="com.canyinghao.canphotos.CanPicturesActivity" />
        <activity android:name="com.canyinghao.canphotos.CanViewPagerActivity" />

        <activity
            android:name="com.yanzhenjie.album.AlbumActivity"
            android:label="图库"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden|stateHidden" />
        <activity android:theme="@style/AppTheme"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|stateHidden"
            android:launchMode="singleTop"
            android:name="activity.LookBigPicActivity"/>
    </application>

</manifest>
